{
  "name": "vendor/my-php-app",
  "description": "A PHP app with MVC pattern and Docker setup",
  "type": "project",
  "require": {
    "php": "^8.1",
    "monolog/monolog": "^2.0",
    "vlucas/phpdotenv": "^5.0",
    "twig/twig": "^3.0",
    "doctrine/migrations": "^3.7",
    "doctrine/dbal": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "symfony/var-dumper": "^6.4"
  },
  "scripts": {
    "test": "phpunit"
  }
}
